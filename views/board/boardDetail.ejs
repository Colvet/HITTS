<% include ../header.ejs %>


    <div class="panel panel-default">
        <div class="panel-heading">
        </div>
        <div class="panel-body">
            <div style="padding-bottom: 10px">
                자료명 : <%=board.title%>
            </div>
            
            <div style="padding-bottom: 10px">
                작성자 : <%=board.writer%>
            </div> 

            <div style="padding-bottom: 10px">
                파일 : 
            <% if(board.record){%>
                <a href="/hitts/board_uploads/<%=board.record%>" /> <%=board.record%> </a>
                
            </div>
            <% } %>

            <div style="padding-bottom: 10px">
                설명 <%-board.description%>
            </div>

            <div style="padding-bottom: 10px">
                작성일 : <%=board.getDate.year%> -
                        <%=board.getDate.month%> -
                        <%=board.getDate.day%>
            </div>

            

            
        <a href="/hitts/board" class="btn btn-default">목록으로</a>
        <a href="/hitts/board/edit/<%=board.id%>" class="btn btn-primary">수정</a>

<% include ../footer.ejs %>